#include<stdio.h>
#include<string.h>
int main()
{
    char str[100];
    int weight=0;
    printf("Enter a string");
    scanf("%s,str");
    
    for(int i=1;str[i]!='\0';i++)
    {
        weight +=(str[i]-'a'+1);
    }
    printf("The total number of weights are:%d",weight);
    
    return 0;
}


#include<stdio.h>
void main()
{
    int n,i,j,count=0;
    {
        printf("Input the number of elements stored in the array:");
        scanf("%d",&n);
        int arr[n];
        printf("Input %d elements in the array;\n",n);
        for(i=0;i<n;i++)
        {
            printf("Element %d:",i);
            scanf("%d",&arr[i]);

        }
        for(i=0;i<n;i++)
        {
        int duplicateFound=0;
            for(j=0;j<i;j++)
            {
                if(arr[i]==arr[j])
                {
                    duplicateFound=0;
                    break;
                }
            }
        }
        
        for(j=i+1;j<n;j++)
        {
            if(arr[i]==arr[j])
            {
                count++;
                
            }
        }
    }
   
}
    
    
    

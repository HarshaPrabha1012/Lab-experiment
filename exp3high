#include<stdio.h>
int main()
{
    for(int i=1;i<=9;i++)
    {
        for(int j=1;j<9;j++)
        {
            if(i==j||j==5-i+1)
            {
                printf("%d",j);
            }
                else
                {
                    printf("");
                    
                }
            }
        }
        printf("/n");
    }

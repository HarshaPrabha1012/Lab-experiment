#include<stdio.h>
void evenodd(int num)
{
    if(num%2==0)
    {
    printf("The entered number is even:\n");
    }
    else
    {
    printf("The entered number is odd:\n");
    }
}

int main()
{
    int num;
    printf("Enter the number:");
    scanf("%d",&num);
    evenodd(num);
    return 0;

}

